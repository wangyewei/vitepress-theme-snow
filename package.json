{
  "packageManager": "pnpm@8.15.4",
  "name": "vitepress-theme-snow",
  "version": "1.0.2-beta.1",
  "description": "A modern blog theme for vitepress",
  "type": "module",
  "engines": {
    "node": ">=18.19.0"
  },
  "exports": {
    ".": "./dist/index.js",
    "./*": "./*"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist/",
    "package.json",
    "README.md",
    "README_ZH.md",
    "LICENSE",
    "ADDITION_TERMS.md",
    "ADDITION_TERMS_ZH.md",
    "DOCUMENTATION.md",
    "SHORT_TERM_GOAL.md"
  ],
  "keywords": [
    "theme",
    "vitepress",
    "tailwind",
    "typescript",
    "tsx",
    "blog",
    "shiro"
  ],
  "author": "Yev Wang",
  "license": "AGPL-3.0-with-conditions",
  "scripts": {
    "dev:tailwind": "npx tailwindcss -i ./src/styles/tailwind.css -o ./src/styles/virtual-tailwind.css --watch",
    "build:tailwind": "npx tailwindcss -i ./src/styles/tailwind.css -o ./src/styles/virtual-tailwind.css",
    "build:css": "pnpm run build:tailwind && esno ./scripts/build-css",
    "build:prepare": "rimraf dist && pnpm run build:clean-cache",
    "build:theme": "rollup --config rollup.config.ts --configPlugin esbuild",
    "build:clean-cache": "rimraf types-temp",
    "build": "run-s build:prepare build:ts build:theme build:css build:clean-cache",
    "build:theme-only": "run-s build:prepare build:theme build:clean-cache",
    "build:ts": "tsc --emitDeclarationOnly --declaration --outDir types-temp",
    "release": "bumpp package.json --commit --push --tag"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-inject": "^5.0.5",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/pluginutils": "^5.1.0",
    "@types/chroma-js": "^2.4.4",
    "@types/fs-extra": "^11.0.4",
    "@types/pngjs": "^6.0.5",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.4.0",
    "@vue/babel-plugin-jsx": "^1.2.2",
    "@vue/tsconfig": "^0.7.0",
    "autoprefixer": "^10.4.19",
    "bumpp": "^9.4.1",
    "daisyui": "^4.11.1",
    "esno": "^4.7.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "postcss-import": "^16.1.0",
    "postcss-nested": "^7.0.2",
    "rimraf": "^6.0.0",
    "rollup": "^4.18.0",
    "rollup-plugin-dts": "^6.1.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-vue": "^6.0.0",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.4.5",
    "unplugin-vue-jsx": "^0.4.0",
    "vite": "^6.0.4",
    "vitepress": "^1.1.4",
    "vue": "^3.4.31"
  },
  "dependencies": {
    "@allindevelopers/vue-floating-ui": "^1.1.1",
    "@floating-ui/vue": "^1.0.6",
    "@tailwindcss/typography": "^0.5.13",
    "@vueuse/motion": "^2.1.0",
    "chroma-js": "^3.0.0",
    "colorjs.io": "^0.5.0",
    "hero-motion": "^0.5.2",
    "pngjs": "^7.0.0",
    "tailwind-scrollbar": "^3.1.0",
    "tailwind-variants": "^0.3.0",
    "vaul-vue": "^0.2.0"
  }
}